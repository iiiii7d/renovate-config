{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>iiiii7d/renovate-config:no-dev"
  ],
  "baseBranchPatterns": ["dev", "$default"],
  "packageRules": [
    {
      "matchBaseBranches": ["master", "main"],
      "enabled": false
    },
    {
      "matchCategories": ["ci"],
      "matchBaseBranches": ["master", "main"],
      "enabled": true
    }
  ]
}
